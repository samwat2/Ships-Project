<h1>Boats#new</h1>
<p>Find me in app/views/boats/new.html.erb</p>
<%= form_with model: @boat, data: {"parsely-validate": ''}, local: true do |f| %>
	<%= f.text_field :name, placeholder: 'name', required: true %>
	<%= f.text_field :location, placeholder: 'location', required: true %>
	<%= f.number_field :containers, placeholder: 'Amount of Containers', required: true %>
	<%= f.submit %>
<% end %>