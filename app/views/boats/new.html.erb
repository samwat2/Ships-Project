<h1>Boats#new</h1>
<p>Find me in app/views/boats/new.html.erb</p>
<%= form_with model: @boat, local: true do |f| %>
	<%= f.text_field :name, placeholder: 'name' %>
	<%= f.text_field :location, placeholder: 'location' %>
	<%= f.number_field :containers, placeholder: 'Amount of Containers' %>
	<%= f.submit %>
<% end %>